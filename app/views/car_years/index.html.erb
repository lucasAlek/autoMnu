<p id="notice"><%= notice %></p>

<h1>Car Years</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_years.each do |car_year| %>
      <tr>
        <td><%= link_to 'Show', car_year %></td>
        <td><%= link_to 'Edit', edit_car_year_path(car_year) %></td>
        <td><%= link_to 'Destroy', car_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Year', new_car_year_path %>
