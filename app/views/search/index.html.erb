<h1>Search</h1>
<p>"<%= @search_trems %>" </p>

<table class="ui celled padded table">
  <thead>
  <tr>
    <th>Model</th>
    <th>Make</th>
    <th>year</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @cars.each do |car| %>
    <tr>
      <td><%= car.model %></td>
      <td><%= car.make %></td>
      <td><%= Year.find(car.year_id_id).year %></td>
      <td><%= link_to 'Show', car, class: 'ui positive basic button' %></td>
      <td><%= link_to 'Edit', edit_car_path(car), class: 'ui primery basic button' %></td>
      <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>