<p id="notice"><%= notice %></p>
<%= will_paginate @cars %>
<h1>Cars</h1>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Make</th>
      <th>year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.model %></td>
        <td><%= car.make %></td>
        <td><%= Year.find(car.year_id_id).year %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @cars %>
<%= link_to 'New Car', new_car_path %>
